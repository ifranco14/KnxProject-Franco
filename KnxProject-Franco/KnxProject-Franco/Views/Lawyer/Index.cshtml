@model IEnumerable<KnxProject_Franco.CONTRACTS.Entities.LawyerModel>

@{
    ViewBag.Title = "Index";
}

<h2>Nosotros</h2>

<div class="fh5co-team-style-3">
    <div class="container">
        <div class="row p-b">
            <div class="col-md-6 col-md-offset-3 text-center">
                <h2 class="fh5co-heading">Nuestro equipo</h2>
                <p>Altamente capacitado e intrínsecamente vinculado a las necesidades de nuestros clientes.</p>
            </div>
        </div>
        <div class="row">
            @foreach (var x in Model)
            {
                <div class="col-md-4 text-center">
                    <div class="person">
                        <img src="images/person_4.jpg" class="img-responsive img-rounded" alt="Person">
                        <h3 class="name">@x.FirstName @x.LastName</h3>
                        <div class="position">{@foreach (var item in ViewBag.CourtBranches)
                        {
                            if (item.ID = @x.CourtBranchId)
                            {
                                @item.Name
                            }
                        }}</div>
                        <p>TEXTO QUE NO SE QUE PONER blaaaaaaaaaaaaaaaa blaaaaaaaaaaaaaaaaaa bla bla bla blaaaaaaaaaaaaaaaaaaaaaaa</p>
                        <ul class="social social-circle">
                            <li><a href="#"><i class="icon-twitter"></i></a></li>
                            <li><a href="#"><i class="icon-linkedin"></i></a></li>
                            <li><a href="#"><i class="icon-instagram"></i></a></li>
                            <li><a href="#"><i class="icon-github"></i></a></li>
                        </ul>
                    </div>
                </div>
            }


        </div>
    </div>
</div>