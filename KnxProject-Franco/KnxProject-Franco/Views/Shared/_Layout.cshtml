<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - K.U. & Asociados</title>
    <!-- Styles -->
    <link rel="stylesheet" href="~/Content/css/style.css">
    @*<!-- Google Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,700|Monsterrat:400,700|Merriweather:400,300italic,700' rel='stylesheet' type='text/css'>*@
    <!-- Animate.css -->
    <link rel="stylesheet" href="~/Content/css/animate.css">
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href="~/Content/css/icomoon.css">
    <!-- Magnific Popup-->
    <link rel="stylesheet" href="~/Content/css/magnific-popup.css">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="~/Content/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/css/owl.theme.default.min.css">
    <!-- Bootstrap  -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.css">
    <!-- Cards -->
    <link rel="stylesheet" href="~/Content/css/cards.css">
    <!--Site-->
    <link rel="stylesheet" href="~/Content/Site.css">
    <!--FontAwesome-->
    <link rel="stylesheet" href="~/Content/css/font-awesome.css">
    <!--FontAwesome.min-->
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css">
    @Scripts.Render("~/bundles/modernizr")
</head>

<body class="fh5co-overflow myBody">    
    <div id="fh5co-page">
        
            <div class="nav navbar-fixed-top fh5co-nav-style-1" role="navigation" data-offcanvass-position="fh5co-offcanvass-left">
                <div class="container">
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 fh5co-logo">
                        <a href="#" class="js-fh5co-mobile-toggle fh5co-nav-toggle"><i></i></a>
                        <a href="/Home/Index"><i class="fa fa-balance-scale"></i></a> <!--aria-hidden="true"-->
                    </div>
                    <div class="col-lg-6 col-md-5 col-sm-5 text-center fh5co-link-wrap">
                        <ul data-offcanvass="yes">
                            <li class="active">@Html.ActionLink("Home", "Index", "Home")</li>
                            <li>@Html.ActionLink("Noticias","Index","News")</li>
                            <li>@Html.ActionLink("Abogados", "IndexLawyers", "Person") </li>
                            <li>@Html.ActionLink("Ramas","Index","CourtBranch")</li>
                            @Html.Partial("_UserOptions")   
                        </ul>
                    </div>

                    @Html.Partial("_LoginPartial")
                </div>
            </div>
        
        <div class="col-md-push-12">
            @RenderBody()
        </div>

    </div>
        <!-- jQuery -->
        <script src="~/Scripts/jquery.min.js"></script>
        <!-- jQuery Easing -->
        <script src="~/Scripts/jquery.easing.1.3.js"></script>
        <!-- Bootstrap -->
        <script src="~/Scripts/bootstrap.min.js"></script>
        <!-- Waypoints -->
    <script src="~/Scripts/jquery.waypoints.min.js"></script>
    @*<!-- Owl Carousel -->
    <script src="~/Scripts/owl.carousel.min.js"></script>*@
    <!-- Magnific Popup -->
    <script src="~/Scripts/jquery.magnific-popup.min.js"></script>
    <!-- Stellar -->
    <script src="~/Scripts/jquery.stellar.min.js"></script>
        <!-- countTo -->
        <script src="~/Scripts/jquery.countTo.js"></script>
        <!-- WOW -->
        <script src="~/Scripts/wow.min.js"></script>
        <!-- Main -->
        <script src="~/Scripts/main.js"></script>
        <!-- @Scripts.Render("~/bundles/jquery")-->
        <!--@Scripts.Render("~/bundles/bootstrap")-->
        @RenderSection("scripts", required: false)
</body>
</html>
